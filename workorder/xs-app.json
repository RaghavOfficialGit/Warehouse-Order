{
  "welcomeFile": "/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/odata/v4/(.*)$",
      "target": "/odata/v4/$1",
      "authenticationType": "none",
      "csrfProtection": false,
      "destination": "srv-api"
    },
    {
      "source": "^/resources/(.*)$",
      "target": "/resources/$1",
      "authenticationType": "none"
    },
    {
      "source": "^/test-resources/(.*)$",
      "target": "/test-resources/$1",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)\\.(.*)$",
      "target": "/$1.$2",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)$",
      "target": "/$1",
      "authenticationType": "none"
    }
  ],
  "cacheControl": [
    {
      "maxAge": 31536000,
      "directive": "public",
      "pattern": "^/resources/"
    },
    {
      "maxAge": 31536000,
      "directive": "public",
      "pattern": "^/test-resources/"
    }
  ]
}
