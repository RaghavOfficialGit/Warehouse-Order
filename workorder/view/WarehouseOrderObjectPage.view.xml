<mvc:View
    xmlns:mdc="sap.ui.mdc"
    xmlns:mdcfield="sap.ui.mdc.field"
    xmlns:form="sap.ui.layout.form"
    xmlns:smartform="sap.ui.comp.smartform"
    xmlns:smartfield="sap.ui.comp.smartfield"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:layout="sap.ui.layout"
    xmlns:table="sap.ui.table"
    xmlns:cd="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    controllerName="sap.fe.templates.ObjectPage.controller.ObjectPageController"
    entitySet="WarehouseOrders">
    
    <mdc:ObjectPage
        id="ObjectPage"
        entitySet="WarehouseOrders"
        useSmartField="true"
        busyIndicatorDelay="0"
        enableAutoBinding="true"
        contextPath="/WarehouseOrders">
        
        <mdc:headerTitle>
            <mdc:ObjectPageHeader
                id="ObjectPageHeader"
                objectTitle="{WarehouseOrder}"
                objectSubtitle="{EWMWarehouse} - {WarehouseOrderStatus}"
                isObjectTitleAlwaysVisible="true"
                isObjectSubtitleAlwaysVisible="true">
                <mdc:actions>
                    <mdc:ObjectPageHeaderActionButton
                        id="editBtn"
                        text="{i18n>edit}"
                        press="onEdit"
                        icon="sap-icon://edit"
                        visible="false"/>
                    <mdc:ObjectPageHeaderActionButton
                        id="refreshBtn"
                        text="{i18n>refresh}"
                        press="onRefresh"
                        icon="sap-icon://refresh"
                        type="Transparent"/>
                </mdc:actions>
            </mdc:ObjectPageHeader>
        </mdc:headerTitle>
        
        <mdc:headerContent>
            <layout:VerticalLayout>
                <layout:HorizontalLayout>
                    <smartform:SmartForm
                        id="headerForm"
                        editable="false"
                        layout="ResponsiveGridLayout"
                        labelSpanXL="3"
                        labelSpanL="3"
                        labelSpanM="3"
                        labelSpanS="12"
                        adjustLabelSpan="false"
                        emptySpanXL="4"
                        emptySpanL="4"
                        emptySpanM="4"
                        emptySpanS="0"
                        columnsXL="1"
                        columnsL="1"
                        columnsM="1"
                        singleContainerFullSize="false">
                        
                        <smartform:Group>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>EWMWarehouse}"
                                    labelFor="EWMWarehouse"/>
                                <smartfield:SmartField
                                    id="EWMWarehouse"
                                    value="{EWMWarehouse}"
                                    editable="false"/>
                            </smartform:GroupElement>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>WarehouseOrder}"
                                    labelFor="WarehouseOrder"/>
                                <smartfield:SmartField
                                    id="WarehouseOrder"
                                    value="{WarehouseOrder}"
                                    editable="false"/>
                            </smartform:GroupElement>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>WarehouseOrderStatus}"
                                    labelFor="WarehouseOrderStatus"/>
                                <smartfield:SmartField
                                    id="WarehouseOrderStatus"
                                    value="{WarehouseOrderStatus}"
                                    editable="false"/>
                            </smartform:GroupElement>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>WhseOrderCreationDateTime}"
                                    labelFor="WhseOrderCreationDateTime"/>
                                <smartfield:SmartField
                                    id="WhseOrderCreationDateTime"
                                    value="{WhseOrderCreationDateTime}"
                                    editable="false"/>
                            </smartform:GroupElement>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>Processor}"
                                    labelFor="Processor"/>
                                <smartfield:SmartField
                                    id="Processor"
                                    value="{Processor}"
                                    editable="false"/>
                            </smartform:GroupElement>
                            <smartform:GroupElement>
                                <smartfield:SmartLabel
                                    text="{i18n>ActivityArea}"
                                    labelFor="ActivityArea"/>
                                <smartfield:SmartField
                                    id="ActivityArea"
                                    value="{ActivityArea}"
                                    editable="false"/>
                            </smartform:GroupElement>
                        </smartform:Group>
                    </smartform:SmartForm>
                </layout:HorizontalLayout>
            </layout:VerticalLayout>
        </mdc:headerContent>
        
        <mdc:sections>
            <mdc:ObjectPageSection
                id="warehouseTasksSection"
                title="Warehouse Tasks"
                titleUppercase="false">
                <mdc:subSections>
                    <mdc:ObjectPageSubSection
                        id="warehouseTasksSubSection"
                        title="">
                        <mdc:blocks>
                            <mdc:Table
                                id="warehouseTasksTable"
                                entitySet="WarehouseTasks"
                                enableAutoBinding="true"
                                personalization="Persist"
                                demandPopin="true"
                                showRowCount="true"
                                enableExport="false"
                                enablePaste="false"
                                selectionMode="None">
                                
                                <mdc:columns>
                                    <mdc:Column
                                        id="WarehouseTaskColumn"
                                        header="{i18n>WarehouseTask}"
                                        importance="High"
                                        width="120px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="WarehouseTaskField"
                                                value="{WarehouseTask}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="WarehouseTaskItemColumn"
                                        header="{i18n>WarehouseTaskItem}"
                                        importance="High"
                                        width="80px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="WarehouseTaskItemField"
                                                value="{WarehouseTaskItem}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="WarehouseTaskStatusColumn"
                                        header="{i18n>WarehouseTaskStatus}"
                                        importance="High"
                                        width="120px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="WarehouseTaskStatusField"
                                                value="{WarehouseTaskStatus}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="ProductColumn"
                                        header="{i18n>Product}"
                                        importance="Medium"
                                        width="150px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="ProductField"
                                                value="{Product}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="QuantityInBaseUnitColumn"
                                        header="{i18n>QuantityInBaseUnit}"
                                        importance="Medium"
                                        width="120px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="QuantityInBaseUnitField"
                                                value="{QuantityInBaseUnit}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="SourceStorageBinColumn"
                                        header="{i18n>SourceStorageBin}"
                                        importance="Medium"
                                        width="150px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="SourceStorageBinField"
                                                value="{SourceStorageBin}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="DestinationStorageBinColumn"
                                        header="{i18n>DestinationStorageBin}"
                                        importance="Medium"
                                        width="150px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="DestinationStorageBinField"
                                                value="{DestinationStorageBin}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                </mdc:columns>
                            </mdc:Table>
                        </mdc:blocks>
                    </mdc:ObjectPageSubSection>
                </mdc:subSections>
            </mdc:ObjectPageSection>
            
            <mdc:ObjectPageSection
                id="handlingUnitsSection"
                title="Handling Units"
                titleUppercase="false">
                <mdc:subSections>
                    <mdc:ObjectPageSubSection
                        id="handlingUnitsSubSection"
                        title="">
                        <mdc:blocks>
                            <mdc:Table
                                id="handlingUnitsTable"
                                entitySet="WarehouseOrderPickHndlgUnits"
                                enableAutoBinding="true"
                                personalization="Persist"
                                demandPopin="true"
                                showRowCount="true"
                                enableExport="false"
                                enablePaste="false"
                                selectionMode="None">
                                
                                <mdc:columns>
                                    <mdc:Column
                                        id="HandlingUnitExternalIDColumn"
                                        header="{i18n>HandlingUnitExternalID}"
                                        importance="High"
                                        width="150px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="HandlingUnitExternalIDField"
                                                value="{HandlingUnitExternalID}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="PackagingMaterialColumn"
                                        header="{i18n>PackagingMaterial}"
                                        importance="Medium"
                                        width="150px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="PackagingMaterialField"
                                                value="{PackagingMaterial}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                    <mdc:Column
                                        id="EWMWhseOrderNmbrOfHndlgUnitsColumn"
                                        header="{i18n>EWMWhseOrderNmbrOfHndlgUnits}"
                                        importance="Medium"
                                        width="180px">
                                        <mdc:field>
                                            <mdcfield:Field
                                                id="EWMWhseOrderNmbrOfHndlgUnitsField"
                                                value="{EWMWhseOrderNmbrOfHndlgUnits}"
                                                editable="false"/>
                                        </mdc:field>
                                    </mdc:Column>
                                </mdc:columns>
                            </mdc:Table>
                        </mdc:blocks>
                    </mdc:ObjectPageSubSection>
                </mdc:subSections>
            </mdc:ObjectPageSection>
        </mdc:sections>
    </mdc:ObjectPage>
</mvc:View>
